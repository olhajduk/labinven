{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },

  "rules": {
    "semi":"off",
    "comma-dangle":"off",
    "require-jsdoc":"off",
    "eol-last":"off",
    "arrow-parens":"off",
    "indent":"off",
    "max-len":"off"
  },

  "extends": [
    "eslint:recommended", 
    "google"
  ]
}